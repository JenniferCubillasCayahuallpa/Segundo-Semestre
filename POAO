//numero4

/**
 *
 * @author Lab4
 */
public class Ejercicio4 {
    public static void main(String[] args) {
      
    float a,b,multiplicación,división,suma,resta;
    a=100;
    b=10;
    multiplicación=a*b;
    división=a/b;
    suma=a+b;
    resta=a-b;
    
    
    
    System.out.println("La suma es: "+suma
    +"\nLa resta es: "+resta
    +"\nLa multiplicación es:"+multiplicación
    +"\nLa división es"+división);

}
}
